{
  "version": 2,
  "name": "gmpi-interfaz-hc",
  "builds": [
    {
      "src": "FRONTEND/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/FRONTEND/html/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/FRONTEND/html/index.html"
    },
    {
      "src": "/dashboard.html",
      "dest": "/FRONTEND/html/dashboard.html"
    },
    {
      "src": "/infraestructuras.html",
      "dest": "/FRONTEND/html/infraestructuras.html"
    },
    {
      "src": "/calendario.html",
      "dest": "/FRONTEND/html/calendario.html"
    },
    {
      "src": "/add-institution.html",
      "dest": "/FRONTEND/html/add-institution.html"
    },
    {
      "src": "/edit-institution.html",
      "dest": "/FRONTEND/html/edit-institution.html"
    },
    {
      "src": "/register.html",
      "dest": "/FRONTEND/html/register.html"
    },
    {
      "src": "/register-inspector.html",
      "dest": "/FRONTEND/html/register-inspector.html"
    },
    {
      "src": "/contact.html",
      "dest": "/FRONTEND/html/contact.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/FRONTEND/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/FRONTEND/js/$1"
    },
    {
      "src": "/javascript/(.*)",
      "dest": "/FRONTEND/javascript/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/FRONTEND/images/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/FRONTEND/$1"
    }
  ]
}
